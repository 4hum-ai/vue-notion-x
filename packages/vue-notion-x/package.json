{
  "name": "vue-notion-x",
  "version": "0.1.0",
  "type": "module",
  "description": "Fast and accurate Vue renderer for Notion.",
  "repository": "yourusername/vue-notion-x",
  "author": "Vue Notion X Contributors",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./styles.css": "./src/styles.css"
  },
  "files": [
    "build",
    "src/styles.css"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --sourcemap --target es2018 --external vue && cp src/styles.css build/styles.css",
    "dev": "tsup --watch",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "clean": "del build",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:typecheck": "vue-tsc --noEmit",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "katex": "catalog:",
    "lodash.throttle": "catalog:",
    "medium-zoom": "catalog:",
    "notion-types": "workspace:*",
    "notion-utils": "workspace:*",
    "prismjs": "catalog:"
  },
  "devDependencies": {
    "@types/lodash.throttle": "catalog:",
    "@types/prismjs": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vue": "catalog:",
    "vue-pdf-embed": "catalog:"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "optionalDependencies": {
    "vue-pdf-embed": "catalog:"
  }
}
